;;; Function called when word not found in lexicon
(define (ucsc_sin_lts_function word features)
 "(ucsc_sin_lts_function WORD FEATURES)
Return pronunciation of word not in lexicon."
 (let ((phones) (entry))
  (set! entry
  (if (lts.in.alphabet word 'sin_charset)
	(list
	 word
	 features
	 ;; Currently ignore syllabification
	;; (list (list (lts.apply word 'sin_charset) 1)))
	(syllabify (lts.apply word 'sin_charset)))
	(begin
	 (format stderr "unpronounciation word %s\n" word)
	 (list word features '(((m aa t aaa ee ee k aaa k i y aaa v aa n n aaa b ae h ae) 1))))))
;;  (format t "%l\n" entry)
  entry))

